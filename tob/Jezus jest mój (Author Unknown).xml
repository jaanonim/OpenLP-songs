  <properties>
    <titles>
      <title>Jezus jest mój</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Wędrowiec" entry="239"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jezus jest mój, Jezus jest mój,<br/>Jezus jest mój, miłuję Go!</lines>
    </verse>
    <verse name="v2">
      <lines>/ Jezus jest mój, miłuję Go /x2</lines>
    </verse>
    <verse name="v3">
      <lines>/ On Królem jest, On Panem jest /x2</lines>
    </verse>
    <verse name="v4">
      <lines>/ On królów Król i panów Pan! /x2</lines>
    </verse>
  </lyrics>
