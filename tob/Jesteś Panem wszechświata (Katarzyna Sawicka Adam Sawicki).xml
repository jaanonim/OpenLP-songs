  <properties>
    <titles>
      <title>Jesteś Panem wszechświata</title>
    </titles>
    <authors>
      <author type="words">Katarzyna Sawicka Adam Sawicki</author>
      <author type="music">Katarzyna Sawicka Adam Sawicki</author>
    </authors>
    <songbooks>
      <songbook name="TomaszKupka" entry="274"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jesteś Panem wszechświata<br/>i Królem mojego życia,<br/>Jezu Tyś Panem mym.<br/>Jesteś Królem wszechświata<br/>i Panem mojego życia<br/>wywyższam Ciebie dziś.</lines>
    </verse>
    <verse name="c1">
      <lines>Wywyższam Cię,<br/>Wywyższam Cię,<br/>Wywyższam Cię.</lines>
    </verse>
  </lyrics>
</song>
