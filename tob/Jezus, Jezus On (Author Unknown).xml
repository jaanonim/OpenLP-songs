  <properties>
    <titles>
      <title>Jezus, Jezus On</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Pieśni Nowego Życia" entry="114"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jezus, Jezus!<br/>On Panem jest,<br/>mój Jezus.</lines>
    </verse>
    <verse name="v2">
      <lines>Jezus, Jezus!<br/>On władzę ma,<br/>mój Jezus.</lines>
    </verse>
    <verse name="v3">
      <lines>Jezus, Jezus!<br/>On przyjdzie wnet,<br/>mój Jezus.</lines>
    </verse>
  </lyrics>
