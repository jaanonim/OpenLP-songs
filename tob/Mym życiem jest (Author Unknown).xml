  <properties>
    <titles>
      <title>Mym życiem jest</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Pieśni Nowego Życia" entry="292"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Mym życiem jest<br/>Sam Chrystus Pan,<br/>A śmierć - to zysk!<br/>To zysk!</lines>
    </verse>
    <verse name="v2">
      <lines>Pan dobry jest,<br/>Miłuje nas<br/>Bez miar! Amen! Amen!<br/>Amen, amen...</lines>
    </verse>
  </lyrics>
</song>
