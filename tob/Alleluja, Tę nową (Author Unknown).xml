<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-12-15T17:47:09">
  <properties>
    <titles>
      <title>Alleluja, Tę nową</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Pieśni Nowego Życia" entry="90"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Alleluja, alleluja,<br/>Tę nową pieśń na Twoją cześć,<br/>Alleluja, alleluja,<br/>śpiewamy Ci, nasz Panie!<br/>Alleluja, alleluja,<br/>Baranek nasz, alleluja !<br/>Alleluja, alleluja!<br/>Godzien jet czci na wieki!</lines>
    </verse>
  </lyrics>
</song>
