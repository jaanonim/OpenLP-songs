  <properties>
    <titles>
      <title>Chrystus jest życiem mym</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Różnymi Pieśniami"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Chrystus jest życiem mym<br/>A śmierć zyskiem jest /x2<br/>To mój Król, moja pieśń<br/>Życie me i radość ma<br/>Moja moc i mój miecz<br/>Pokój mój, tym jest mój Pan</lines>
    </verse>
  </lyrics>
</song>
