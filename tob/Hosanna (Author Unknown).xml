<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-12-15T17:48:06">
  <properties>
    <titles>
      <title>Hosanna</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Różnymi Pieśniami"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>/ Hosanna, Hosanna,<br/>Królowi, królów cześć /x2</lines>
    </verse>
    <verse name="c1">
      <lines>Chwalmy Pana śpiew niech brzmi,<br/>Zmartwychwstał Król i żyje dziś,</lines>
    </verse>
    <verse name="v2">
      <lines>/ Chwalmy Go najwyższy On<br/>i Godzien wszelkiej czci /x2</lines>
    </verse>
    <verse name="v3">
      <lines>/ Wielbimy, wielbimy,<br/>Wielbimy Panie Cię /x2</lines>
    </verse>
    <verse name="v4">
      <lines>/ Kochamy, kochamy,<br/>Kochamy Panie Cię /x2</lines>
    </verse>
  </lyrics>
</song>
