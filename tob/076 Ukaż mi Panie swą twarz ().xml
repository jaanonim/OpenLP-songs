      <title>076 Ukaż mi Panie swą twarz</title>
    </titles>
    <ccliNo>No.</ccliNo>
    <authors>
      <author/>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ukaż mi Panie swą twarz <br/>Daj mi usłyszeć Twój głos<br/>Naucz mnie szukać Ciebie<br/>wskaż mi cień Twego oblicza</lines>
    </verse>
  </lyrics>
