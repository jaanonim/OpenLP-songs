  <properties>
    <titles>
      <title>279 W TOBIE JEST ŚWIATŁO</title>
    </titles>
    <authors>
      <author>()</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>W Tobie jest światło<br/>każdy mrok rozjaśnia<br/>W Tobie jest życie<br/>Ono śmierć zwycięża<br/>Ufam Tobie, miłosierny<br/>Jezu wybaw nas</lines>
    </verse>
  </lyrics>
