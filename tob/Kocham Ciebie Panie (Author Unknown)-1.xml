<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-12-15T17:48:08">
  <properties>
    <titles>
      <title>Kocham Ciebie Panie</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Różnymi Pieśniami"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Kocham Ciebie Panie i uwielbiam<br/>Wznoszę w gore swoje ręce<br/>Uwielbiając Imię Twe /x2</lines>
    </verse>
    <verse name="v2">
      <lines>Bo wielkimś Ty<br/>Wielkie dzieła czynisz dziś<br/>Nie dorówna Tobie nikt<br/>Nie dorówna Tobie nikt /x2</lines>
    </verse>
  </lyrics>
</song>
