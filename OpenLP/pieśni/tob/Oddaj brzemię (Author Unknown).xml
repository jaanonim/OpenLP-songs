  <lyrics>
    <verse name="v1">
      <lines>Oddaj brzemię Jezusowi,<br/>On weźmie je. // x2 //</lines>
    </verse>
    <verse name="c1">
      <lines>Wyżej, wyżej,<br/>wyżej, wyżej,<br/>wyżej, wyżej<br/>Jezus coraz wyżej. // x2 //</lines>
    </verse>
    <verse name="v2">
      <lines>Przerzuć troski na Jezusa,<br/>On weźmie je. // x2 //</lines>
    </verse>
  </lyrics>
