  <properties>
    <titles>
      <title>Oddaj brzemię</title>
      <title>Cast your burdens</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="TomaszKupka" entry="471"/>
    </songbooks>
