  <properties>
    <titles>
      <title>Każdy może dziś</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Głos Wiary" entry="513"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>// Każdy może dziś do Jezusa przyjść //<br/>Każdy może dziś do Jezusa przyjść,<br/>On nie odrzuci cię.</lines>
    </verse>
    <verse name="c1">
      <lines>// Jezus, Jezus, Jezus, Jezus leczy chore serca //<br/>Jezus, Jezus, Jezus, Jezus leczy chore serca,<br/>On pomoże ci!</lines>
    </verse>
  </lyrics>
</song>
