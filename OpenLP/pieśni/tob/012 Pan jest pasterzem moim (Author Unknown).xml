  <lyrics>
    <verse name="v1">
      <lines>Pan jest pasterzem moim, niczego mi nie braknie. <br/>Na niwach zielonych pasie mnie,<br/>nad wody spokojne prowadzi mnie</lines>
    </verse>
  </lyrics>
